{
  "manifest_version": 3,
  "name": "Obed Filter",
  "description": "Obed Filter фильтрует товары на страницах ресторанов сайта obed.ru",
  "version": "1.0",
  "icons": {
    "16": "icons/logo-16.png",
    "32": "icons/logo-32.png",
    "48": "icons/logo-48.png",
    "128": "icons/logo-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.obed.ru/restaurant/*"
      ],
      "js": [
        "index.js"
      ],
      "css": [
        "style.css"
      ]
    }
  ]
}
